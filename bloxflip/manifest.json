{
  "name": "Bloxflip injection",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Inject the tcheat console!",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],

  "action": {
    "default_popup": "main.html"
  }
}